model User {
  id          String    @id @default(cuid())
  name        String
  email       String    @unique
  phoneNumber String    @unique
  password    String
  role        String    @default("user")
  createdAt   DateTime  @default(now())
  Address     Address[]
}

model SocialLink {
  id   String @id @default(cuid())
  // vendorId Int
  type String
  url  String

  // vendor Vendor @relation(fields: [vendorId], references: [id])
}
