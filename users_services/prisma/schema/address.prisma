model Address {
  id          String  @id @default(cuid())
  userId      String?
  sellerId   String? 
  road        String?
  house       String?
  district    String?
  division    String?
  country     String?
  description String?

  User  User? @relation(fields: [userId], references: [id])
  Seller Seller? @relation(fields: [sellerId], references: [id])
  // Orders Orders[]
}
