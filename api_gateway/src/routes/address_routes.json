{
  "address": {
    "url": "http://localhost:3001/api",
    "routes": [
      {
        "path": "v1/address/crud/create",
        "method": ["POST"],
        "middlewares": ["auth"],
        "role": ["user"]
      },
      {
        "path": "v1/address/crud/single/:id",
        "method": ["GET"],
        "middlewares": ["auth"],
        "role": ["user", "admin", "seller"]
      },
      {
        "path": "v1/address/crud/update/:id",
        "method": ["PUT"],
        "middlewares": ["auth"],
        "role": ["user"]
      },
      {
        "path": "v1/address/crud/delete/:id",
        "method": ["DELETE"],
        "middlewares": ["auth"],
        "role": ["user"]
      },
      {
        "path": "v1/address/crud/all-by-user/:userId",
        "method": ["GET"],
        "middlewares": ["auth"],
        "role": ["user", "admin"]
      }
    ]
  }
}
