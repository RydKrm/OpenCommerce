{
  "wishlist": {
    "url": "http://localhost:3001/api",
    "routes": [
      {
        "path": "v1/wishlist/create",
        "method": ["POST"],
        "middlewares": ["auth"],
        "role": ["user"]
      },
      {
        "path": "v1/wishlist/list-by-user/:id",
        "method": ["GET"],
        "middlewares": ["auth"],
        "role": ["user"]
      },
      {
        "path": "v1/wishlist/delete/:userId",
        "method": ["DELETE"],
        "middlewares": ["auth"],
        "role": ["user"]
      },
      {
        "path": "v1/wishlist/all",
        "method": ["GET"],
        "middlewares": [],
        "role": []
      },
      {
        "path": "v1/wishlist/by-user",
        "method": ["GET"],
        "middlewares": ["auth"],
        "role": ["user"]
      },
      {
        "path": "v1/wishlist/by-product/:productId",
        "method": ["GET"],
        "middlewares": ["auth"],
        "role": ["user"]
      }
    ]
  }
}
